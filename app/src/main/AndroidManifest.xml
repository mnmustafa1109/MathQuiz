<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mustafa.mathquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Addition"
            android:label="Addition">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mustafa.mathquiz.MainActivity" />
        </activity>
        <activity
            android:name=".Subtraction"
            android:label="Subtraction">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mustafa.mathquiz.MainActivity" />
        </activity>
        <activity
            android:name=".Multiplication"
            android:label="Multiplication">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mustafa.mathquiz.MainActivity" />
        </activity>
        <activity
            android:name=".Division"
            android:label="Divison">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mustafa.mathquiz.MainActivity" />
        </activity>

        <service
            android:name=".MusicService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".Main_Music_Service"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>